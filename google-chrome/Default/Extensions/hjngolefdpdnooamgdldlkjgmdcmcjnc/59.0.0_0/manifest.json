         "suggested_key": {
            "chromeos": "Alt+Shift+Q",
            "linux": "Alt+Shift+Q",
            "mac": "Alt+Shift+Q",
            "windows": "Alt+Shift+Q"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "content/chromeBundle.js" ],
      "matches": [ "https://docs.google.com/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "content/officeBundle.js" ],
